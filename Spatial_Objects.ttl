:ProximaCentauri_b a :Planet.

:ProximaCentauri_c a :Planet.

:InternationalAstronomicalUnion a dbo:Organisation;
     rdfs:label "International Astronomical Union".

#Constellations

:Zodiac
    :hasLocation dbo:Ecliptic;
    :shape "circle";
    :ZodiacConstellations[
       a rdf:Bag;
        rdf:_1  :Taurus;
        rdf:_2 :Virgo;
        rdf:_3 :Gemini;
        rdf:_4 :Cancer;
        rdf:_5 :Aquarius;
        rdf:_6 :Aries;
        rdf:_7 :Capricornus;
        rdf:_8 :Leo;
        rdf:_9 :Libra;
        rdf:_10 :Pisces;
        rdf:_11 :Sagittarius;
        rdf:_12 :Scorpius].

#StarClusters

:Pleiades a :OpenCluster;
    rdfs:label "Pleiades";
    :knowAs "Seven Sisters and Messier 45"^^xsd:string;
    :hasLocation :Taurus;
    :distance "444"^^dbo:LightYear;
    :ApparentMagnitude "1.6"^^xsd:decimal.

:Hyades a :OpenCluster;
    rdfs:label "Hyades";
    :knowAs "CaldWell 41, Collinder 50, Melotte 25"^^xsd:string;
    :hasLocation :Taurus;
    :distance "153"^^dbo:LightYear;
    :ApparentMagnitude "0.5"^^xsd:decimal;
    :mass "400"^^dbr:Solar_Mass.

:47Tucanae a :GlobularCluster;
    rdfs:label "47 Tucanae";
    :hasLocation dbo:Tucana;
    :ApparentMagnitude "4.09"^^xsd:decimal;
    :distance "13000"^^dbo:LightYear.
