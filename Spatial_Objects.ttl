@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix schema: <https://schema.org/docs/schemas.html>.
@prefix dc: <http://dublincore.org/documents/dcmi-terms/>.
@prefix event: <http://motools.sourceforge.net/event/event.html>.
@prefix time: <https://www.w3.org/TR/owl-time/>.
@prefix dbo: <http://dbpedia.org/ontology/>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <https://www.w3.org/2001/XMLSchema/#>.

@prefix : <https://inf8410.com/tp/astronomical-object/>.

#########################
#       Classes         #
#########################

:AstronomicalObject a rdfs:Class;
    rdfs:subClassOf :deepSkyObject.

:Galaxy a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody;
    :ourGalaxy :MilkyWay;
    :isMadeOf :GalaxiesComponents;
    :famousGalaxies
    [
    rdf:type rdf:Bag;
    rdf:_1 :MilkyWay;
    rdf:_2 :LMC;
    rdf:_3 :Andromeda;
    rdf:_4 :CigarGalaxy;
    rdf:_5 :PinwheelGalaxy
    ].

:Definition a rdfs:Class.

:Planet a rdfs:Class;
   rdfs:subClassOf dbo:CelestialBody.

:Swarm a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody.

:Star a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody.

:ArtificalSatellite a rdf:Class;
    rdfs:label "PlanetarySystems";
    rdfs:subClassOf dbo:Satellite.

:Asteroid a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody.

:Satellite
    rdfs:subClassOf dbo:CelestialBody.

:PlanetarySystems a rdfs:Class;
    rdfs:label "Planetary Systems";
    rdfs:subClassOf dbo:CelestialBody.

:BlackHole a rdfs:Class;
    rdfs:label "Black Hole";
    rdfs:subClassOf dbo:CelestialBody;
    :isDefinedBy
    [
    rdf:type rdf:Bag;
    rdf:_1 :SpacetimeRegion;
    rdf:_2 :Gravity
    ];
    :mostMassive
    [
    rdf:type rdf:Bag;
    rdf:_1 :TON618;
    rdf:_2 :Holmberg15A;
    rdf:_3 :IC1101
    ].

:fundamentalUnitsOfUniverseStructure a rdfs:Class.

:Matter a rdfs:Class.

:KnownMatter a :Class;
    rdfs:label "Known Matter";
    rdf:subClassOf :Matter.

:KnownPhysics a rdfs:Class;
    rdfs:label "Known Physics".


:ConstellationFamily a rdfs:Class;
    rdfs:subClassOf :Constellation;
    :collectionOf
            (:UrsaMajor :Zodiac :Perseus :Hercules :Orion
                :HeavenlyWaters :Bayer :LaCaille).


:Constellation a rdfs:Class;
      rdfs:subClassOf :CelestialBody;
      :count "88"^^xsd:integer;
      :recognizedBy :InternationalAstronomicalUnion;
      :recognitionYear "1922"^^xsd:integer.


#########################
#   Objects Properties  #
#########################

:ourGalaxy a rdf:Property;
    rdfs:subPropertyOf :UniverseElements;
    rdfs:comment "Contains our solar system";
    rdfs:domain :Galaxy;
    rdfs:range :fundamentalUnitsOfUniverseStructure.

:isDefinedBy a rdf:Property;
    rdfs:subPropertyOf :Description;
    rdfs:comment "Elements that describe a BlackHole";
    rdfs:domain :Definition;
    rdfs:range :KnownPhysics.

:Distance a rdf:Property;
    rdfs:comment "Distance from Earth, in light year".

:Mass a rdf:Property;
    rdfs:range :AstronomicalObject;
    rdfs:comment "Object Mass".

:isMadeOf a rdf:Property;
    rdfs:subPropertyOf :UniverseElements;
    rdfs:comment "Elements that compose a galaxy";
    rdfs:domain :Matter;
    rdfs:range :KnownMatter.

#########################
#      Instances        #
#########################

:GalaxiesComponents a rdf:Bag;
    rdf:_1 :Stars;
    rdf:_2 :InterstellarGas;
    rdf:_3 :Dust;
    rdf:_4 :Emptiness;
    rdf:_5 :DarkMatter.

#Galaxies
:MilkyWay a dbo:Galaxy;
    rdfs:label "MilkyWay";
    :name "Milky Way";
    :containsOur :SolarSystem;
    :diameter "1E5"^^dbo:LightYear;
    :Distance "0"^^xsd:integer.

:LMC a dbo:Galaxy;
    rdfs:label "Large Magellanic Cloud";
    :name "LMC";
    :diameter "1.4E4"^^dbo:LightYear;
    :Distance "1.58E5"^^xsd:double.

:Andromeda a dbo:Galaxy;
    rdfs:label "Andromeda";
    :name "Andromeda";
    :diameter "2.2E5"^^dbo:LightYear;
    :Distance "2.5E6"^^xsd:double.

:CigarGalaxy a dbo:Galaxy;
    rdfs:label "Cigar Galaxy";
    :name "Cigar Galaxy";
    :diameter "3.7E5"^^dbo:LightYear;
    :Distance "1.15E7"^^xsd:double.

:PinwheelGalaxy a dbo:Galaxy;
    rdfs:label "Pinwheel Galaxy";
    :name "Pinwheel Galaxy";
    :diameter "1.7E5"^^dbo:LightYear;
    :Distance "2.087E7"^^xsd:double.

#Stars
:Sun a :Star;
    rdfs:label "Sun";
    :ApparentMagnitude "-26.74"^^xsd:decimal;
    :Distance "1.6E-5"^^xsd:double;
    :Mass "1.0"^^dbo:SolarMass.

:AlphaCentauriA a :Star;
    rdfs:label "Rigil Kentaurus";
    :ApparentMagnitude "-0.01"^^xsd:decimal;
    :Distance "4,365"^^xsd:double;
    :Mass "1.1"^^dbo:SolarMass.

:AlphaCentauriB a :Star;
    rdfs:label "Rigil Kentaurus B";
    :ApparentMagnitude "1,35"^^xsd:decimal;
    :Distance "4,365"^^xsd:double;
    :Mass "0.907"^^dbo:SolarMass.

:AlphaCentauriC a :Star;
    rdfs:label "Proxima Centauri";
    :ApparentMagnitude "15.60"^^xsd:decimal;
    :Distance "4.244"^^xsd:double;
    :Mass "0.122"^^dbo:SolarMass.


# BlackHoles
:TON618 a :BlackHole;
    :name "TON 618";
    :Mass "6.6E10"^^dbo:SolarMass;
    :howMassWasObtain "Estimated from quasar HB emission line correlation".

:Holmberg15A a :BlackHole;
    :name "Holmberg 15 A";
    :Mass "4.0E10"^^dbo:SolarMass;
    :howMassWasObtain "Mass specified obtained through orbit-based, axisymmetric Schwarzschild models".

:IC1101 a :BlackHole;
    :name "IC 1101";
    :Mass "7E10"^^dbo:SolarMass;
    :howMassWasObtain "Estimated from properties of the host galaxy; mass has not been measured directly".

#PlanetarySystems
:SolarSystem a :PlanetarySystems;
    rdfs:label "Solar System";
    :hasStar :Sun;
    :Planets
        (:Mercury :Venus :Earth :Mars :Jupyter :Saturn :Uranus :Neptune).

:AlphaCentauri a :PlanetarySystems;
    rdfs:label "Alpha Centauri";
    :hasStar :AlphaCentauriA;
    :hasStar :AlphaCentauriB;
    :hasStar :AlphaCentauriC;
    :Planets
        (:ProximaCentauri_b :ProximaCentauri_c).


#Planets

:Mercury a :Planet;
    :Mass "..".
:Venus a :Planet.
:Earth a :Planet;
    :Satellites
        (:Moon).
:Mars a :Planet;
    :Satellites
        (:Phobos :Deimos).
:Jupyter a :Planet;
    :Satellites
        (:Io :Europa :Ganymede :Callisto).
:Saturn a :Planet;
    :Satellites
        (:Pan :Daphnis :Atlas :Prometheus :Pandora :Epimetheus :Janus :Mimas :Methone :Anthe :Pallene :Enceladus :Tethys :Telesto :Calypso :Dione :Helene :Polydeuces :Rhea :Titan :Hyperion :Iapetus :Kiviuq :Ijiraq :Phoebe :Paaliaq :Skathi :Albiorix :Bebhionn :Erriapo :Skoll :Siarnaq :Tarqeq
         :Greip :Hyrrokkin :Jarnsaxa :Tarvos :Mundilfari :Bergelmir :Narvi :Suttungr :Hati :Farbauti :Thrymr :Aegir :Bestla :Fenrir :Surtur :Kari :Ymir :Loge :Fornjot).
:Uranus a :Planet;
    :Satellites
        (:Puck :Miranda :Ariel :Umbriel :Titania :Ob√©ron).
:Neptune a :Planet;
    :Satellites
        (:Triton).

:ProximaCentauri_b a :Planet.

:ProximaCentauri_c a :Planet.

:InternationalAstronomicalUniona a :organisation.


:Zodiac
    a :Place;
    :shape "circle";
    :madeOf :Constellation;
    :idk[
       a rdf:Bag;
        rdf:_1  :Taurus;
        rdf:_2 :Virgo;
        rdf:_3 :Gemini;
        rdf:_4 :Cancer;
        rdf:_5 :Aquarius;
        rdf:_6 :Aries;
        rdf:_7 :Capricornus;
        rdf:_8 :Leo;
        rdf:_9 :Libra;
        rdf:_10 :Pisces;
        rdf:_11 :Sagittarius;
        rdf:_12 :Scorpius].
