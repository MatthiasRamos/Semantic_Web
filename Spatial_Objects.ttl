@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix foaf: <http://xmlns.com/foaf/spec/>.
@prefix schema: <https://schema.org/docs/schemas.html>.
@prefix dc: <http://dublincore.org/documents/dcmi-terms/>.
@prefix event: <http://motools.sourceforge.net/event/event.html>.
@prefix time: <https://www.w3.org/TR/owl-time/>.
@prefix dbo: <http://dbpedia.org/ontology/>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix : <https://inf8410.com/tp/astronomical-object#>.
@prefix xsd: <https://www.w3.org/2001/XMLSchema/#>.

@prefix : <https://inf8410.com/tp/astronomical-object/>.

#########################
#       Classes         #
#########################

:AstronomicalObject a rdfs:Class;
    rdfs:subClassOf :deepSkyObject.

:Galaxy a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody;
    :ourGalaxy :MilkyWay;
    :isMadeOf :GalaxiesComponents;
    :famousGalaxies
    [
    rdf:type rdf:Bag;
    rdf:_1 :MilkyWay;
    rdf:_2 :LMC;
    rdf:_3 :Andromeda;
    rdf:_4 :CigarGalaxy;
    rdf:_5 :PinwheelGalaxy
    ].

:Definition a rdfs:Class.

:Planet a rdfs:Class;
   rdfs:subClassOf dbo:CelestialBody.

:Swarm a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody.

:Star a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody.

:ArtificalSatellite a rdf:Class;
    rdfs:subClassOf dbo:Satellite.

:Asteroid a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody.

:Satellite
    rdfs:subClassOf dbo:CelestialBody.

:PlanetarySystems a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody.

:BlackHoles a rdfs:Class;
    rdfs:subClassOf dbo:CelestialBody;
    :isDefinedBy
    [
    rdf:type rdf:Bag;
    rdf:_1 :SpacetimeRegion;
    rdf:_2 :Gravity
    ];
    :mostMassive
    [
    rdf:type rdf:Bag;
    rdf:_1 :TON618;
    rdf:_2 :Holmberg15A;
    rdf:_3 :IC1101
    ].

:fundamentalUnitsOfUniverseStructure a rdfs:Class.

:Matter a rdfs:Class.

:KnownMatter a :Class;
    rdf:subClassOf :Matter.

:KnownPhysics a rdfs:Class.

:LightYear a dbo:Light-year.

:SolarMass a dbo:Solar_mass.

#########################
#   Objects Properties  #
#########################

:ourGalaxy a rdf:Property;
    rdfs:subPropertyOf :UniverseElements;
    rdfs:comment "Contains our solar system";
    rdfs:domain :Galaxy;
    rdfs:range :fundamentalUnitsOfUniverseStructure.

:isDefinedBy a rdf:Property;
    rdfs:subPropertyOf :Description;
    rdfs:comment "Elements that describe a BlackHole";
    rdfs:domain :Definition;
    rdfs:range :KnownPhysics.

:Distance a rdf:Property;
    rdfs:comment "Distance from Earth, in light year".

:Mass a rdf:Property;
    rdfs:range :AstronomicalObject;
    rdfs:comment "Object Mass".

:isMadeOf a rdf:Property;
    rdfs:subPropertyOf :UniverseElements;
    rdfs:comment "Elements that compose a galaxy";
    rdfs:domain :Matter;
    rdfs:range :KnownMatter.

#########################
#      Instances        #
#########################

:GalaxiesComponents a rdf:Bag;
    rdf:_1 :Stars;
    rdf:_2 :InterstellarGas;
    rdf:_3 :Dust;
    rdf:_4 :Emptiness;
    rdf:_5 :DarkMatter.

#Galaxies
:MilkyWay a dbo:Galaxy;
    :name "Milky Way";
    :containsOur :SolarSystem;
    :diameter "1E5"^^dbo:LightYear;
    :Distance "0"^^xsd:integer.

:LMC a dbo:Galaxy;
    :name "LMC";
    :diameter "1.4E4"^^dbo:LightYear;
    :Distance "1.58E5"^^xsd:double.

:Andromeda a dbo:Galaxy;
    :name "Andromeda";
    :diameter "2.2E5"^^dbo:LightYear;
    :Distance "2.5E6"^^xsd:double.

:CigarGalaxy a dbo:Galaxy;
    :name "Cigar Galaxy";
    :diameter "3.7E5"^^dbo:LightYear;
    :Distance "1.15E7"^^xsd:double.

:PinwheelGalaxy a dbo:Galaxy;
    :name "Pinwheel Galaxy";
    :diameter "1.7E5"^^dbo:LightYear;
    :Distance "2.087E7"^^xsd:double.

# BlackHoles
:TON618 a :BlackHoles;
    :name "TON 618";
    :Mass "6.6E10"^^:SolarMass;
    :howMassWasObtain "Estimated from quasar HB emission line correlation".

:Holmberg15A a :BlackHoles;
    :name "Holmberg 15 A";
    :Mass "4.0E10"^^:SolarMass;
    :howMassWasObtain "Mass specified obtained through orbit-based, axisymmetric Schwarzschild models".

:IC1101 a :BlackHoles;
    :name "IC 1101";
    :Mass "7E10"^^:SolarMass;
    :howMassWasObtain "Estimated from properties of the host galaxy; mass has not been measured directly".

#PlanetarySystems
:SolarSystem a :PlanetarySystems;
    :hasStar :Sun;
    :ApparentMagnitude "-26.74"^^xsd:decimal;
    :ConfirmedPlanets "8"^^xsd:integer;
    :Distance "1.6E-5"^^xsd:double;
    :Mass 1.0;
    :Planets
        (:Mercury :Venus :Earth :Mars :Jupyter :Saturn :Uranus :Neptune).

#Planets

:Mercury a :Planet;
    :Mass "..".
:Venus a :Planet.
:Earth a :Planet;
    :Satellites
        (:Moon).
:Mars a :Planet;
    :Satellites
        (:Phobos :Deimos).
:Jupyter a :Planet;
    :Satellites
        (:Io :Europa :Ganymede :Callisto).
:Saturn a :Planet;
    :Satellites
        (:Pan :Daphnis :Atlas :Prometheus :Pandora :Epimetheus :Janus :Mimas :Methone :Anthe :Pallene :Enceladus :Tethys :Telesto :Calypso :Dione :Helene :Polydeuces :Rhea :Titan :Hyperion :Iapetus :Kiviuq :Ijiraq :Phoebe :Paaliaq :Skathi :Albiorix :Bebhionn :Erriapo :Skoll :Siarnaq :Tarqeq
         :Greip :Hyrrokkin :Jarnsaxa :Tarvos :Mundilfari :Bergelmir :Narvi :Suttungr :Hati :Farbauti :Thrymr :Aegir :Bestla :Fenrir :Surtur :Kari :Ymir :Loge :Fornjot).
:Uranus a :Planet;
    :Satellites
        (:Puck :Miranda :Ariel :Umbriel :Titania :Ob√©ron).
:Neptune a :Planet;
    :Satellites
        (:Triton).

:Zodiac
    a :Place;
    :shape "circle";
    :madeOf :Constellation;
    :idk[
       a rdf:Bag;
        rdf:_1  :Taurus;
        rdf:_2 :Virgo;
        rdf:_3 :Gemini;
        rdf:_4 :Cancer;
        rdf:_5 :Aquarius;
        rdf:_6 :Aries;
        rdf:_7 :Capricornus;
        rdf:_8 :Leo;
        rdf:_9 :Libra;
        rdf:_10 :Pisces;
        rdf:_11 :Sagittarius;
        rdf:_12 :Scorpius].
